{
  "name": "visa-acceptance-agent-toolkit-ai-sdk-example",
  "version": "0.1.0",
  "description": "Example using Visa Acceptance AgentToolkit with AI SDK",
  "scripts": {
    "start": "ts-node index.ts",
    "check": "ts-node env-check.ts",
    "direct": "ts-node --transpile-only direct-run.ts",
    "list": "ts-node --transpile-only direct-list.ts",
    "debug": "ts-node --transpile-only debug-sandbox.ts",
    "nl:invoice": "ts-node --transpile-only nl-invoice.ts",
    "nl:invoice:get": "ts-node --transpile-only nl-invoice-get.ts",
    "nl:invoice:send": "ts-node --transpile-only nl-invoice-send.ts",
    "nl:invoice:cancel": "ts-node --transpile-only nl-invoice-cancel.ts",
    "nl:invoice:update": "ts-node --transpile-only nl-invoice-update.ts",
    "nl:pl": "ts-node --transpile-only nl-paymentlink.ts",
    "nl:pl:get": "ts-node --transpile-only nl-paymentlink-get.ts",
    "nl:pl:update": "ts-node --transpile-only nl-paymentlink-update.ts",
    "nl:interactive": "ts-node --transpile-only interactive-nl.ts",
    "nl:invoice:list": "ts-node --transpile-only nl-invoice-list.ts",
    "nl:pl:list": "ts-node --transpile-only nl-paymentlink-list.ts",
    "pl:create": "npm run direct",
    "pl:create:direct": "ts-node --transpile-only direct-paymentlink-create.ts",
    "pl:list": "npm run list",
    "pl:get": "ts-node --transpile-only direct-paymentlink-get.ts",
    "pl:update": "ts-node --transpile-only direct-paymentlink-update.ts",
    "inv:create": "ts-node --transpile-only direct-invoice-create.ts",
    "inv:list": "ts-node --transpile-only direct-invoice-list.ts",
    "inv:get": "ts-node --transpile-only direct-invoice-get.ts",
    "inv:update": "ts-node --transpile-only direct-invoice-update.ts",
    "inv:send": "ts-node --transpile-only direct-invoice-send.ts",
    "inv:cancel": "ts-node --transpile-only direct-invoice-cancel.ts",
    "debug:all": "npm run debug",
    "smoke": "ts-node --transpile-only smoke.ts",
    "smoke:direct": "node smoke-run.js",
    "test:smoke": "jest --runInBand",
    "cleanup": "ts-node --transpile-only cleanup.ts"
  },
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/openai": "^1.3.21",
    "@visaacceptance/agent-toolkit": "^0.0.94",
    "ai": "^4.3.13",
    "colors": "^1.4.0",
    "cybersource-rest-client": "^0.0.67",
    "dotenv": "^16.5.0",
    "superagent": "^10.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "jest": "^29.7.0"
  }
}